QComboBox {
	width: 150px;
	height: 25px;
	border-radius: 3px;
	
	font: 15px;
	font-family:"System";

}

QComboBox::drop-down {
    subcontrol-origin: padding;
    subcontrol-position: top right;
    width: 20px;
 
    border-left-width: 1px;
    border-left-color: darkgray;
    border-left-style: solid; /* just a single line */
    border-top-right-radius: 3px; /* same radius as the QComboBox */
    border-bottom-right-radius: 3px;
}
 
QComboBox::down-arrow {
    image: url(./image/arrow_down_normal.png);
}



QComboBox QAbstractItemView  
{  
	border-radius: 3px;
	border:none;
	background:#f5f5f5;	
}  

QComboBox QAbstractItemView::focus
{  
     outline: none;
}
  
QComboBox QAbstractItemView::item  
{  
    height: 25px;  
}  
  
QComboBox QAbstractItemView::item:selected  
{     
	background:#2CA7F8;	
}  

QPushButton#commucation_sure_pushButton {
	background-color: #A4D3EE;
	border-style: outset;
	border-width:1px;
	border-radius:5px;
	border-color: #dddddd;
	font:Bold 20px;
	font-family: "仿宋";
	min-width:10em;
	padding:6px;
}
QPushButton#commucation_sure_pushButton:pressed {
	background-color: #9AC0CD;
	border-style: inset;
}